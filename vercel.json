{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "server/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "outputDirectory": "dist",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "server/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": "server/"
    }
}